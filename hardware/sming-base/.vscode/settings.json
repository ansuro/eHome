{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.{c|cpp|h|hpp}$",
                "isAsync": true,
                "cmd": "gtags ${workspaceRoot}"
            }
        ]
    },
    "files.associations": {
        "*.js": "javascriptreact",
        "functional": "cpp",
        "user_config.h": "c"
    }
}